{
    "type": "origins:multiple",
    "loop": {
        "type": "origins:action_over_time",
        "interval": 1,
        "condition": {
            "type": "origins:constant",
            "value": true
        },
        "rising_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:command",
                "command": "execute if entity @s[tag=TSMP.Titan]",
                "comparison": "==",
                "compare_to": 1
            },
            "if_action": {
                "type": "origins:if_else",
                "condition": {
                    "type": "origins:command",
                    "command": "execute if entity @s[tag=TSMP.HugeTitan]",
                    "comparison": "==",
                    "compare_to": 1
                },
                "if_action": {
                    "type": "origins:execute_command",
                    "command": "scale set pehkui:held_item 0.25",
                    "permission_level": 4
                },
                "else_action": {
                    "type": "origins:execute_command",
                    "command": "scale set pehkui:held_item 1",
                    "permission_level": 4
                }
            },
            "else_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:execute_command",
                        "command": "clear @s #phantom:warhammer_hardened_items{WarhammerHardenedItem:1}",
                        "permission_level": 4
                    }
                ]
            }
        },
        "falling_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:command",
                        "command": "execute if entity @s[tag=TSMP.Titan]",
                        "comparison": "==",
                        "compare_to": 1
                    },
                    "if_action": {
                        "type": "origins:if_else",
                        "condition": {
                            "type": "origins:command",
                            "command": "execute if entity @s[tag=TSMP.HugeTitan]",
                            "comparison": "==",
                            "compare_to": 1
                        },
                        "if_action": {
                            "type": "origins:execute_command",
                            "command": "scale set pehkui:held_item 0.03",
                            "permission_level": 4
                        },
                        "else_action": {
                            "type": "origins:execute_command",
                            "command": "scale set pehkui:held_item 0.14",
                            "permission_level": 4
                        }
                    }
                },
                {
                    "type": "origins:execute_command",
                    "command": "clear @s #phantom:warhammer_hardened_items{WarhammerHardenedItem:1}",
                    "permission_level": 4
                }
            ]
        },
        "entity_action": {
            "type": "origins:if_else",
            "condition": {
                "type": "origins:command",
                "command": "execute if entity @s[tag=TSMP.Titan]",
                "comparison": "==",
                "compare_to": 0
            },
            "if_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:execute_command",
                        "command": "scale set pehkui:held_item 1",
                        "permission_level": 4
                    },
                    {
                        "type": "origins:execute_command",
                        "command": "clear @s #phantom:warhammer_hardened_items{WarhammerHardenedItem:1}",
                        "permission_level": 4
                    }
                ]
            }
        }
    },
    "callback": {
        "type": "origins:action_on_callback",
        "entity_action_gained": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "scale delay set pehkui:held_item 0",
                    "permission_level": 4
                },
                {
                    "type": "origins:delay",
                    "ticks": 2,
                    "action": {
                        "type": "origins:execute_command",
                        "command": "scale delay set pehkui:held_item 0",
                        "permission_level": 4
                    }
                }
            ]
        },
        "entity_action_lost": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:command",
                        "command": "execute if entity @s[tag=TSMP.Titan]",
                        "comparison": "==",
                        "compare_to": 1
                    },
                    "if_action": {
                        "type": "origins:if_else",
                        "condition": {
                            "type": "origins:command",
                            "command": "execute if entity @s[tag=TSMP.HugeTitan]",
                            "comparison": "==",
                            "compare_to": 1
                        },
                        "if_action": {
                            "type": "origins:execute_command",
                            "command": "scale set pehkui:held_item 0.03",
                            "permission_level": 4
                        },
                        "else_action": {
                            "type": "origins:execute_command",
                            "command": "scale set pehkui:held_item 0.14",
                            "permission_level": 4
                        }
                    },
                    "else_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:execute_command",
                                "command": "scale set pehkui:held_item 1",
                                "permission_level": 4
                            }
                        ]
                    }
                },
                {
                    "type": "origins:delay",
                    "ticks": 1,
                    "action": {
                        "type": "origins:execute_command",
                        "command": "scale delay set pehkui:held_item 20",
                        "permission_level": 4
                    }
                }
            ]
        }
    },
    "titan_skin_crossbow_arrow_no_pickup": {
        "type": "origins:active_self",
        "key": {
            "key": "key.use"
        },
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:equipped_item",
                        "equipment_slot": "mainhand",
                        "item_condition": {
                            "type": "origins:and",
                            "conditions": [
                                {
                                    "type": "origins:ingredient",
                                    "ingredient": {
                                        "item": "appli:titan_skin_crossbow"
                                    }
                                },
                                {
                                    "type": "origins:nbt",
                                    "nbt": "{Charged:1b}"
                                }
                            ]
                        }
                    },
                    "if_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:delay",
                                "ticks": 5,
                                "action": {
                                    "type": "origins:area_of_effect",
                                    "radius": 30,
                                    "bientity_condition": {
                                        "type": "origins:and",
                                        "conditions": [
                                            {
                                                "type": "appli:nbt",
                                                "actor_nbt": "UUID",
                                                "target_nbt": "Owner"
                                            },
                                            {
                                                "type": "origins:target_condition",
                                                "condition": {
                                                    "type": "origins:entity_type",
                                                    "entity_type": "minecraft:arrow"
                                                }
                                            },
                                            {
                                                "type": "origins:target_condition",
                                                "condition": {
                                                    "type": "origins:nbt",
                                                    "nbt": "{OnGround:0b}"
                                                }
                                            }
                                        ]
                                    },
                                    "bientity_action": {
                                        "type": "origins:and",
                                        "actions": [
                                            {
                                                "type": "origins:target_action",
                                                "action": {
                                                    "type": "origins:execute_command",
                                                    "command": "data modify entity @s pickup set value 0b"
                                                }
                                            }
                                        ]
                                    }
                                }
                            }
                        ]
                    }
                }
            ]
        }
    }
}